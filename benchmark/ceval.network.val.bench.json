                "function choice(response) {",
                "    var choices = [\"A\", \"B\", \"C\", \"D\"];",
                "    for (var i = 0; i < choices.length; i++) {",
                "        if(response.indexOf(choices[i]) != -1) {",
                "            return choices[i];",
                "        }",
                "    }",
                "    return \"None\";",
                "}"
            ],
            "info": "extract A/B/C/D or None"
        }
